{
  "name": "aas-paas-backend",
  "version": "1.0.0",
  "description": "Backend API for Aas Paas - Hyperlocal vendor-customer platform",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "seed": "node src/scripts/seed.js"
  },
  "keywords": [
    "mern",
    "hyperlocal",
    "vendor",
    "customer"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "aas-paas-backend": "file:",
    "aws-sdk": "^2.1501.0",
    "cloudinary": "^1.41.3",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "node-geocoder": "^4.2.0",
    "nodemailer": "^6.9.7",
    "sharp": "^0.33.1",
    "tesseract.js": "^5.0.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "mongodb-memory-server": "^10.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": ["<rootDir>/src/test/jest.setup.js"],
    "testMatch": ["**/src/**/?(*.)+(spec|test).js"]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
